{
    "compilerOptions": {
      "target": "es2020",
      "module": "Node16",
      "lib": ["es2020", "dom"],
      "noEmit": true,
      "strict": false,
      "moduleResolution": "Node16",
      "resolveJsonModule": true
    },
    "exclude": ["node_modules"],
    "include": [
      "src/*.js",
      "test/*.js",
      "src/**/*.d.ts"
    ]
}
